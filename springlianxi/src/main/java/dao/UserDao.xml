<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.UserDao">

    <select id="selectUsers" resultType="user.User">
       select userid,username,userpassword,email,sex from examsystem order by userid
    </select>

    <insert id="insertUser">
        insert into examsystem values(#{userid},#{username},#{userpassword},#{email},#{sex})
    </insert>
</mapper>
